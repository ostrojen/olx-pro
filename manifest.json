{
    "manifest_version": 2,
    "name": "OLX Pro",
    "description": "Дозволяє не відволікатися на рекламу при пошуку на olx.ua",
    "version": "0.1.0",
    "author": "ostrojen1996",
    "icons":
    {
        "48": "icons/logo48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.olx.ua/*"],
            "run_at": "document_start",
            "js": ["/js/top.js"]
        }
    ]
}